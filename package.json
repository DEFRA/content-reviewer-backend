{
  "name": "content-reviewer-backend",
  "version": "0.0.0",
  "description": "content-reviewer-backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run server:watch",
    "dev:debug": "cross-env NODE_ENV=development npm run server:debug",
    "docker:dev": "npm run dev",
    "format": "npx prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "npx prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run security-audit && npm run format:check",
    "lint": "npx eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npx eslint --cache --cache-strategy content \"**/*.{cjs,js}\" --fix",
    "postinstall": "npm run setup:husky",
    "security-audit": "npm audit --production --audit-level=high",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "cross-env NODE_ENV=production node --use-strict .",
    "test": "npx cross-env TZ=UTC vitest run --coverage",
    "test:watch": "npx cross-env TZ=UTC vitest"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.980.0",
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/client-sqs": "^3.980.0",
    "@defra/cdp-auditing": "0.6.0",
    "@defra/hapi-secure-context": "0.4.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.4",
    "aws-embedded-metrics": "4.2.1",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dotenv": "17.2.3",
    "global-agent": "3.0.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.6",
    "joi": "18.0.2",
    "mammoth": "1.11.0",
    "mime-types": "3.0.2",
    "mongo-locks": "3.1.2",
    "mongodb": "7.0.0",
    "multer": "2.0.2",
    "node-fetch": "3.3.2",
    "pdf-parse": "2.4.5",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "undici": "7.20.0",
    "uuid": "13.0.0"
  },
  "overrides": {
    "fast-xml-parser": ">=5.3.6",
    "ajv": ">=8.18.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "10.4.23",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "neostandard": "0.12.2",
    "nodemon": "3.1.11",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.4",
    "vitest": "4.0.16",
    "vitest-fetch-mock": "0.4.5",
    "vitest-mongodb": "1.0.3"
  }
}
